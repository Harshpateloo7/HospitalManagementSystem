@model HospitalManagementSystem.Models.PatientDto
@{
    ViewBag.Title = "View";
}

<a href="/Patient/List">Go back to list</a>
<br />
<a href="/Patient/Edit/@Model.PatientId">Update Patient Details</a>
<h2>@Model.PatientName</h2>
<div>Phone No: @Model.PatientPhone</div>
<div>Email: @Model.PatientEmail</div>
<div>Prescription Details: @Model.PatientPrescription</div>
<div>@Model.PatientAddress</div>
<div>@Model.BranchName</div>
<br />
<a href="/Patient/DeleteConfirm/@Model.PatientId">Delete</a>


